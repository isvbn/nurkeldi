#include<iostream>
#include<cmath>
#include<string>
using namespace std;
int main(){
    int a;
    cin>>a;
    if(a==1 || a==2 || a==12)
        cout<<"WINTER";
    if(a==3 || a==4 || a==5)
        cout<<"SPRING";
    if(a==6 || a==7 || a==8)
        cout<<"SUMMER";
    if(a==9 || a==10 || a==11)
        cout<<"FALL";
    }
